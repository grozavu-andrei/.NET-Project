@model IEnumerable<Data.Core.Domain.Cerere>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nume)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Prenume)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.InitialaTatalui)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumarMatricol)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SerieBuletin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumarBuletin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.StareCerere)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prenume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InitialaTatalui)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumarMatricol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SerieBuletin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumarBuletin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StareCerere)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
